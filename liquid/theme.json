{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"layout": {
			"contentSize": "1024px",
			"wideSize": "1280px"
		},
		"color": {
			"background": true,
			"text": true,
			"link": true,
			"custom": true,
			"defaultPalette": true,
			"palette": [],
			"customGradient": true,
			"defaultGradients": true,
			"gradients": [],
			"duotone": [],
			"customDuotone": true,
			"defaultDuotone": true
		},
		"spacing": {
			"blockGap": null,
			"margin": true,
			"padding": true,
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"dropCap": true,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": true,
			"textDecoration": true,
			"textTransform": true,
			"customFontSize": true,
			"fluid": true,
			"fontSizes": [
				{
					"fluid": {
						"min": "0.7rem",
						"max": "0.8rem"
					},
					"size": "0.8rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "0.9rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.25rem",
						"max": "1.5rem"
					},
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "2rem"
					},
					"size": "2rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "3rem",
						"max": "10rem"
					},
					"size": "10rem",
					"slug": "xx-large"
				}
			],
			"fontFamilies": [
				{
					"slug": "serif",
					"name": "serif",
					"fontFamily": "serif"
				},
				{
					"slug": "sans-serif",
					"name": "sans-serif",
					"fontFamily": "sans-serif"
				}
			]
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		}
	}
}